<section class="container signup">
  <h2 class="inactive"> sign in </h2>
  <h2 class="active"> sign up </h2>

  <form class="form signup">
    <div>
      <input type="text" id="firstname-signup"  class="text" placeholder="first name" />
    </div>
    <div>
      <input type="text" id="lastname-signup" class="text" placeholder="last name" />
    </div>
    <div>
      <input type="text" id="address-signup" class="text" placeholder="address" />
    </div>
    <div>
      <input type="text" id="email-signup" class="text" placeholder="email" />
    </div>
    <div>
      <input type="text" id="username-signup" class="text" placeholder="username" />
    </div>
    <div>
      <input type="password" id="password-signup" class="text" placeholder="password" />
    </div>
    <div>
      <button class="signup" type="signup">sign up</button>
    </div>
  </form>
  <script src="/javascript/signup.js"></script>

</div>